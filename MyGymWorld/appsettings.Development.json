{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "Identity": {
    "SignIn": {
      "RequiredConfirmedAccount": false
    },
    "Password": {
      "RequiredLowercase": false,
      "RequiredUppercase": false,
      "RequiredNonAlphanumeric": false,
      "RequiredDigit": true,
      "RequiredLength": 4
    }
  }
}